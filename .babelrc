{
  "presets": [
    "react",
    "flow",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions",
          "node": "current"
        }
      }
    ]
  ],
  "plugins": ["transform-class-properties",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "react-hot-loader/babel"]
}
